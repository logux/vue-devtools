{
  "name": "@logux/vue-devtools",
  "version": "0.2.0",
  "description": "Vue devtools plugin for debugging Logux applications",
  "keywords": [
    "logux",
    "client",
    "vue",
    "devtools",
    "vue-devtools",
    "plugin"
  ],
  "scripts": {
    "test": "eslint . && check-dts"
  },
  "author": "Eduard Aksamitov <e@edaks.co>",
  "license": "MIT",
  "repository": "logux/vue-devtools",
  "sideEffects": false,
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^12.0.0 || ^14.0.0 || >=16.0.0"
  },
  "peerDependencies": {
    "@logux/client": "^0.14.3",
    "@logux/core": "^0.7.2",
    "nanostores": "^0.5.6",
    "vue": ">=3.2.22"
  },
  "dependencies": {
    "@vue/devtools-api": "^6.0.0-beta.20"
  },
  "devDependencies": {
    "@logux/client": "^0.14.3",
    "@logux/core": "^0.7.2",
    "@logux/eslint-config": "^46.1.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "check-dts": "^0.6.4",
    "eslint": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-promise": "^5.1.1",
    "prettier": "^2.4.1",
    "typescript": "^4.5.2",
    "vue": "^3.2.22"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config/esm"
  },
  "eslintIgnore": [
    "**/errors.ts"
  ]
}

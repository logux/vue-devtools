{
  "name": "@logux/vue-devtools",
  "version": "0.0.0",
  "description": "Vue devtools plugin for debugging Logux applications",
  "keywords": [
    "logux",
    "client",
    "vue",
    "devtools",
    "vue-devtools",
    "plugin"
  ],
  "scripts": {
    "test": "eslint . && check-dts && yaspeller *.md"
  },
  "author": "Eduard Aksamitov <e@edaks.co>",
  "license": "MIT",
  "repository": "logux/vue-devtools",
  "sideEffects": false,
  "types": "./index.d.ts",
  "type": "module",
  "engines": {
    "node": "^12.0.0 || >=14.0.0"
  },
  "peerDependencies": {
    "@logux/client": "logux/client#next",
    "@logux/core": "logux/core#next",
    "vue": ">=3.0.11"
  },
  "dependencies": {
    "@vue/devtools-api": "^6.0.0-beta.8"
  },
  "devDependencies": {
    "@logux/client": "logux/client#next",
    "@logux/core": "logux/core#next",
    "@logux/eslint-config": "^45.4.1",
    "@logux/sharec-config": "^0.10.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "check-dts": "^0.4.4",
    "eslint": "^7.25.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.1.0",
    "eslint-plugin-prettierx": "^0.17.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^31.0.0",
    "lint-staged": "^10.5.4",
    "typescript": "^4.2.4",
    "vue": "^3.0.10",
    "yaspeller": "^7.0.0",
    "prettier": "^2.2.1",
    "simple-git-hooks": "^2.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yaspeller",
    "*.js": "eslint",
    "*.ts": "eslint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config/ts"
  },
  "eslintIgnore": [
    "**/errors.ts"
  ],
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)."
    ],
    "dictionary": [
      "versioning",
      "devtools",
      "GraphQL",
      "roadmap",
      "logux",
      "vue"
    ]
  },
  "sharec": {
    "config": "@logux/sharec-config",
    "version": "0.8.5"
  }
}
